# Purpose: Testing numbers (ex: factorial)

# The program is "lexically" correct and should not generate any error 


num: int = int(input("Enter a number: "))
factorial: int = 1

for (i = 0, i < num, i++)
	factorial = factorial*i

print("The factorial of " + num + " is " + factorial)
